<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Metadata>
		[HostComponent("ee.spark.components.support.MenuSeparator")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="dragging" />
		<s:State name="hovered" />
		<s:State name="hoveredAndShowsCaret" basedOn="hovered" />
		<s:State name="normal" />
		<s:State name="normalAndShowsCaret" basedOn="normal" />
		<s:State name="selected" />
		<s:State name="selectedAndShowsCaret" basedOn="selected" />
	</s:states>	
	
	<s:Rect width="100%" height="2" top="2" bottom="2">
		<s:fill>
			<s:SolidColor color="0xCCCCCC" />
		</s:fill>
	</s:Rect>
	
</s:Skin>
